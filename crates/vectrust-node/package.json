{
  "name": "vectrust",
  "version": "0.1.1",
  "description": "High-performance Rust implementation of Vectrust vector database",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Andrey Vasilevsky",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anvanster/vectrust.git",
    "directory": "crates/vectrust-node"
  },
  "keywords": [
    "vector",
    "database",
    "search",
    "similarity",
    "rust",
    "napi"
  ],
  "files": [
    "index.js",
    "index.d.ts",
    "*.node"
  ],
  "napi": {
    "name": "vectrust",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-unknown-linux-musl",
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "test": "node test/test.js"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.14.0",
    "uuid": "^11.1.0"
  }
}